---
    interface Props {
        title: string
    }

    const { title } = Astro.props

    import '../css/animate.css'
    import '../css/custom-animate.css'
    import '../css/flaticon.css';
    import '../css/font-awesome.css'
    import '../css/hover.css'
    import '../css/jquery-ui.css'
    import '../css/jquery.bootstrap-touchspin.css'
    import '../css/jquery.bxslider.min.css'
    import '../css/jquery.fancybox.min.css'
    import '../css/jquery.mCustomScrollbar.min.css'
    import '../css/main.css';
    import '../css/owl.css'
    import '../css/responsive.css';
    import '../css/swiper.css'

    
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="imfoee logo azul.svg" type="image/x-icon">
    <link rel="icon" href="imfoee logo azul.svg" type="image/x-icon">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <title>infoee - {title}</title>
</head>
<body>

    <slot/>

    <script is:inline src="jquery.js"></script>
    <script is:inline src="popper.min.js"></script>
    <script is:inline src="jquery.scrollTo.js"></script>
    <script is:inline src="jquery.mCustomScrollbar.concat.min.js"></script>
    <script is:inline src="jquery.fancybox.js"></script>
    <script is:inline src="appear.js"></script>
    <script is:inline src="swiper.min.js"></script>
    <script is:inline src="element-in-view.js"></script>
    <script is:inline src="jquery.paroller.min.js"></script>
    <script is:inline src="validate.js"></script>

    <script is:inline src="parallax.min.js"></script>
    <script is:inline src="tilt.jquery.min.js"></script>
    
    <script is:inline src="jquery.easing.min.js"></script>
    <script is:inline src="owl.js"></script>
    <script is:inline src="wow.js"></script>
    <script is:inline src="jquery-ui.js"></script>
    <script is:inline src="script.js"></script>
</body>
</html>