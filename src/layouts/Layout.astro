---
    interface Props {
        title?: string
    }

    const { title } = Astro.props

    import '../css/animate.css'
    import '../css/custom-animate.css'
    import '../css/flaticon.css';
    import '../css/font-awesome.css'
    import '../css/hover.css'
    import '../css/jquery-ui.css'
    import '../css/jquery.bootstrap-touchspin.css'
    import '../css/jquery.bxslider.min.css'
    import '../css/jquery.fancybox.min.css'
    import '../css/jquery.mCustomScrollbar.min.css'
    import '../css/main.css';
    import '../css/owl.css'
    import '../css/responsive.css';
    import '../css/swiper.css'

    
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="language" content="spanish">
    <meta name="copyright" content="imfoee">
    <meta name="audience" content="all">
    <meta name="description" content="Empresa dedicada a la capacitación y motivación empresarial. Conoce nuestros cursos de alta calidad para mejorar tus habilidades empresariales.">
    <meta name="keywords" content="motivacion, cursos, cursos empresariales">
    <meta name="robots" content="index, all, follow">
    <meta name="category" content="habilidades empresariales">
    <link rel="shortcut icon" href="imfoee logo azul.svg" type="image/x-icon">
    <link rel="icon" href="imfoee logo azul.svg" type="image/x-icon">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title>imfoee - {title}</title>
</head>
<body>

    <slot/>

    <script is:inline src="/js/form.js"></script>
    <script is:inline src="/js/jquery.js"></script>
    <script is:inline src="/js/popper.min.js"></script>
    <script is:inline src="/js/jquery.scrollTo.js"></script>
    <script is:inline src="/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script is:inline src="/js/jquery.fancybox.js"></script>
    <script is:inline src="/js/appear.js"></script>
    <script is:inline src="/js/swiper.min.js"></script>
    <script is:inline src="/js/element-in-view.js"></script>
    <script is:inline src="/js/jquery.paroller.min.js"></script>
    <script is:inline src="/js/validate.js"></script>

    <script is:inline src="/js/parallax.min.js"></script>
    <script is:inline src="/js/tilt.jquery.min.js"></script>
    
    <script is:inline src="/js/jquery.easing.min.js"></script>
    <script is:inline src="/js/owl.js"></script>
    <script is:inline src="/js/wow.js"></script>
    <script is:inline src="/js/jquery-ui.js"></script>
    <script is:inline src="/js/script.js"></script>
</body>
</html>